                <owl:allValuesFrom rdf:resource="&ssn;System"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ssn;hasOperatingRange"/>
