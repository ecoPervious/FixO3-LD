    <!-- http://www.esonetyellowpages.com/vocab/AcousticDopplerCurrentProfiler -->

    <owl:Class rdf:about="http://www.esonetyellowpages.com/vocab/AcousticDopplerCurrentProfiler">
        <rdfs:label xml:lang="en">Acoustic Doppler Current Profiler</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.esonetyellowpages.com/vocab/HydroacousticCurrentMeter"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ssn;detects"/>
                <owl:allValuesFrom rdf:resource="http://www.esonetyellowpages.com/vocab/DopplerEffect"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Acoustic_Doppler_current_profiler</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">An acoustic Doppler current profiler (ADCP) is a hydroacoustic current meter similar to a sonar, attempting to measure water current velocities over a depth range using the Doppler effect of sound waves scattered back from particles within the water column.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.esonetyellowpages.com/vocab/CurrentMeter -->

    <owl:Class rdf:about="http://www.esonetyellowpages.com/vocab/CurrentMeter">
        <rdfs:label xml:lang="en">Current Meter</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.esonetyellowpages.com/vocab/OceanographicDevice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ssn;observes"/>
                <owl:allValuesFrom rdf:resource="http://www.esonetyellowpages.com/vocab/FlowVelocity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Current_meter</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">A current meter is an oceanographic device for flow measurement by mechanical (rotor current meter), tilt (Tilt Current Meter), acoustical (ADCP) or electrical means.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.esonetyellowpages.com/vocab/DopplerEffect -->

    <owl:Class rdf:about="http://www.esonetyellowpages.com/vocab/DopplerEffect">
        <rdfs:label xml:lang="en">Doppler Effect</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ssn;Stimulus"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ssn;isProxyFor"/>
                <owl:allValuesFrom rdf:resource="http://www.esonetyellowpages.com/vocab/FlowVelocity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Doppler_effect</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">The Doppler effect (or the Doppler shift) is the change in frequency of a wave (or other periodic event) for an observer moving relative to its source.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.esonetyellowpages.com/vocab/FlowVelocity -->

